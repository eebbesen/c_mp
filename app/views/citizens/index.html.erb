<p id="notice"><%= notice %></p>

<h1>Citizens</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Clmp number</th>
      <th>Email</th>
      <th>Phone 1</th>
      <th>Phone 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @citizens.each do |citizen| %>
      <tr>
        <td><%= link_to "#{citizen.first_name} #{citizen.last_name}", edit_citizen_path(citizen) %></td>
        <td><%= citizen.clmp_number %></td>
        <td><%= citizen.email %></td>
        <td><%= citizen.phone_1 %></td>
        <td><%= citizen.phone_2 %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

